  ],
  "author": "Wagner Almeida <wdmeida@gmail.com> (https://www.github.com/wdmeida)",
  "bugs": {
    "url": "https://github.com/wdmeida/react-twitter-login/issues"
  },
  "homepage": "https://github.com/wdmeida/react-twitter-login#readme",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.14.0",
    "webpack": "^4.0.0",
    "webpack-dev-server": "^3.0.0"
  }
